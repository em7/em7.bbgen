<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>


<VBox xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="emko.bbgenui.wordlist.WordListPresenter">
   <children>
      <TableView fx:id="wordsTableView" editable="true">
        <columns>
          <TableColumn fx:id="wordTableColumn" editable="true" text="Word" />
          <TableColumn fx:id="probabilityTableColumn" editable="true" text="Probability" />
          <TableColumn fx:id="actionsTableColumn" editable="false" maxWidth="80.0" minWidth="80.0" text="Actions" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
